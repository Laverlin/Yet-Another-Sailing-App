<settings>

    <setting propertyKey="@Properties.appVersion" title="@Strings.AppVersionTitle">
        <settingConfig type="alphaNumeric" readonly="true" />
    </setting>

    <setting propertyKey="@Properties.userId" title="@Strings.UserIdTitle">
        <settingConfig type="alphaNumeric" required="true" />
    </setting>

    <setting propertyKey="@Properties.timerValue" title="@Strings.TimerValueTitle">
        <settingConfig type="numeric" />
    </setting>

    <setting propertyKey="@Properties.IsAutoRecording" title="@Strings.IsAutoRecordingTitle">
        <settingConfig type="boolean" />
    </setting>
    
    <setting propertyKey="@Properties.isWhiteBackground" title="@Strings.isWhiteBackgroundTitle">
        <settingConfig type="boolean" />
    </setting>
	
</settings>